{% extends 'baseAdmin.html.twig' %}

{% block title %}User{% endblock %}

{% block body %}
    <h1>User</h1>
<div class="card">
    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ user.id }}</td>
            </tr>
            <tr>
                <th>Username</th>
                <td>{{ user.username }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ user.email }}</td>
            </tr>
            <tr>
                <th>Roles</th>
                <td>{{ user.roles ? user.roles|json_encode : '' }}</td>
            </tr>
            
        </tbody>
    </table>
</div>
<br>
 
 
    {{ include('user/_delete_form.html.twig') }}
{% endblock %}
